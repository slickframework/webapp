{
  "name": "slick/webapp",
  "description": "Basic template for a web application built with the Slick framework.",
  "minimum-stability": "dev",
  "license": "MIT",
  "version": "v2.1.0",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\App\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "slick/template": "^3.0@dev",
    "slick/webstack": "^2.0@dev"
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4@dev"
  }
}
